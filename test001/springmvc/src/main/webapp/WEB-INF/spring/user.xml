<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName">

	

	<!-- Root Context: defines shared resources visible to all other web components -->
	<!-- <bean id="user" class="vo.User" scope="prototype"> </bean> -->
	<bean id="studentTest" class="jsk.vo.Student" scope="singleton">
		<property name="studentid" value="id1" />
		<property name="studentname" value="name1" />
	</bean>
	<bean id="userTest" class="jsk.vo.User" scope="singleton"
		init-method="init" destroy-method="destroy">
		<property name="id" value="id1" />
		<property name="name" value="name1" />
	</bean>
	
	<bean id="userDaoTest" class="jsk.dao.UserDao">
	
	</bean>
	<bean id="serviceTest" class="jsk.service.UserService" scope="singleton">
	</bean>
</beans>
